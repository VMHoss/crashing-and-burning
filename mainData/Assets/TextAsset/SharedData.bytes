	Default	[#FoV: 50.0, #Offset: [0.0,3.0,5.0]]
Camera_End

Player_Begin
	// (SS) Multiple Player1 entries here, we might add the globals.Platform string behind each to set the platform specific key inputs, could be added to engine (and template project)
	// PC (webplayer)
	Player1PC			[#Input: [InputKeyboard, InputKeyboardAlt, InputKeyboardAlt2, InputMouse]]
	// Touch devices iOS/Android
	Player1Android		[#Input: [InputKeyboard, InputKeyboardAlt, InputKeyboardAlt2]]
	Player1iOS			[#Input: [InputKeyboard, InputKeyboardAlt, InputKeyboardAlt2]]
Player_End

Input_Begin
	// Note property Type is not used in actions, by default Type can be either Keyboard or Touch
	InputKeyboard		[#Type: Keyboard, #Forward:   Up, #Backward: Down, #Left: Left, #Right: Right, #Drift: Space, #Reset: R, #Fire: Z, #UseItem: X]
	InputKeyboardAlt	[#Type: Keyboard, #Forward:    W, #Backward:    S, #Left:    A, #Right:     D, #Drift: Left Shift,       #Fire: Y]  // Added the Y as fire button for German keyboards layout (requested by KaiserGames)
	InputKeyboardAlt2	[#Type: Keyboard,                                                              #Drift: Right Shift]
	InputMouse			[#Type:    Mouse                                                                                       , #Fire: 0, #UseItem: 1]
	//InputTouch		[#Type:    Touch, #Jump: SwipeUp, #Action: SwipeDown, #DebugLongTap: LongTap]
Input_End

